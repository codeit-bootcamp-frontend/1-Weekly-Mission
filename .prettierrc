{
  "arrowParens": "always",
  "bracketSpacing": true,
  "printWidth": 180,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "tabWidth": 2,
  "importOrder": ["^@/components/(.*)$", "^@/hooks/(.*)$", "^@/services/(.*)$", "^@/store/(.*)$", "^@/utils/(.*)$", "^@/types/(.*)$", "^@/assets/(.*)$", "^[./]"],
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"]
}
