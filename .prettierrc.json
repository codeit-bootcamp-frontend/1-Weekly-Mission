{
  "tabWidth": 2,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "importOrder": [
    "^@/hooks/(.*)$",
    "^@/utils/(.*)$",
    "^@/services/(.*)$",
    "^@/types/(.*)$",
    "^@/pages/(.*)$",
    "^@/containers/(.*)$",
    "^@/components/(.*)$",
    "^@/styles/(.*)$",
    "^@/public/(.*)$",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
